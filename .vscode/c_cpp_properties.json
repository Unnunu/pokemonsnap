{
    "configurations": [
        {
            "name": "Linux",
            "compilerPath": "/usr/bin/gcc",
            "compilerArgs": [
                "-m32"
            ],
            "intelliSenseMode": "${default}",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/src",
                "${workspaceFolder}/build",
                "${workspaceFolder}/include",
                "${workspaceFolder}/build/assets"
            ],
            "defines": [
                "_LANGUAGE_C",
                "NON_MATCHING",
                "F3DEX_GBI_2",
                "_MIPS_SIM=1"
            ],
            "cStandard": "gnu89"
        }
    ],
    "version": 4
}